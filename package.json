{
  "name": "whereat-teams-plugin",
  "version": "1.0.0",
  "description": "Microsoft Teams Tab App for visualizing team geographical distribution",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack.config.js --mode production",
    "build:server": "tsc -p tsconfig.server.json",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "ts-node-dev --transpile-only --respawn -P tsconfig.server.json src/server/index.ts",
    "dev:client": "webpack serve --config webpack.config.js --mode development",
    "start": "node dist/server/index.js",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "@azure/search-documents": "^12.0.0",
    "@fluentui/react-components": "^9.46.0",
    "@fluentui/react-icons": "^2.0.224",
    "@microsoft/teams-js": "^2.19.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.4",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
